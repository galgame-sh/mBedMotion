#include mbed.h
DigitalOut led1(LED1);
AnalogIn DSensor(p15);
int main()
{
    
    while(1){
        
        wait(0.1);
        
        
        if (DSensor = 0.2)
        {
        led1 = 0;
        }
        else
        {
        led1 = 1;
        }        
    
    }
}


